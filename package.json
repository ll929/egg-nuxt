{
  "name": "egg-nuxt",
  "version": "1.0.0",
  "description": "a simple egg-nuxt demo",
  "private": false,
  "scripts": {
    "start": "npm run build && egg-scripts start --daemon --title=egg-server-example",
    "stop": "egg-scripts stop --title=egg-server-example",
    "dev": "egg-bin dev",
    "build": "nuxt build",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --fix --format=node_modules/eslint-formatter-pretty --quiet",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "nuxt": "^1.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "pre-commit": "^1.2.2",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "liulei",
  "license": "MIT"
}
